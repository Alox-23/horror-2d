        self.plant_dat = [[(random.randint(0, len(self.tiles[0])*32), random.randint(0, len(self.tiles)*32)),"plain"] for i in range(100)]

    def create_dat(self):
        for num, row in enumerate(self.tiles):
            y = num * 32
            dat = []
        for index, tile in enumerate(row):
                x = index * 32
                dat.append([(x,y), tile])

        for nu,i in enumerate(dat):
            for j in sorted(dat, key = lambda i : i[1]):
                print(j[1], end = "")
                if 0 in j[1]:
                    print("Yes dady!!")
            print()

m = Map()
m.create_dat()
